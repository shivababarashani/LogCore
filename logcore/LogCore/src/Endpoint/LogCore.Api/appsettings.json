{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "LogDbConStr": "Data Source = .; Initial Catalog = LogCoreDb; Integrated Security = SSPI; MultipleActiveResultSets = True; TrustServerCertificate=True",
  },
  "LogFileConfig": {
    "RootFolder": "logs/",
    "LogFileName": "/log-.log",
    "LogFolderFormat": "yyyy-MM-dd",
    "RollingType": "Day",
    "FileSizeLimitBytes": "1048576",
    "RollOnFileSizeLimit": "true",
    "RetainedFileCountLimit": "null"
  }

}
